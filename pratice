node {
    stage('test') {
        sh 'echo hello'
    }
    stage('learning') {
        git url: 'https://github.com/spring-projects/spring-petclinic.git' ,
            branch: 'main'
    }
}


pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                sh 'echo hello'
            }
        }
        stage('learning') {
            steps {
                git url: 'https://github.com/spring-projects/spring-petclinic.git' , 
                    branch: 'main'
            }
        }
    }
}
